{"ast":null,"code":"import { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default {\n  name: 'Graph',\n  props: ['oeconomicas', 'year_month'],\n  data() {\n    return {\n      balanceBarData: [],\n      barData: {},\n      options: {\n        responsive: false,\n        indexAxis: 'y',\n        maintainAspectRatio: false\n        // scales: {\n        //     yAxes: [{\n        //         ticks: {\n        //         beginAtZero: true,\n        //         },\n        //     }],\n        // },\n      }\n    };\n  },\n\n  created: function () {\n    var outTotal = 0;\n    var inTotal = 0;\n    for (var index = 0; index < this.oeconomicas.length; index++) {\n      var amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n      if (this.oeconomicas[index]['balance'] == \"支出\") {\n        var outTotal = outTotal + Number(amount);\n      } else if (this.oeconomicas[index]['balance'] == \"収入\") {\n        var inTotal = inTotal + Number(amount);\n      }\n    }\n    this.balanceBarData = [inTotal, outTotal];\n    this.setBarData();\n    console.log(this.barData);\n  },\n  mounted() {\n    const ctx = document.getElementById('myChart').getContext('2d');\n    this.myChart = new Chart(ctx, {\n      type: 'bar',\n      data: this.barData,\n      options: this.options\n    });\n  },\n  methods: {\n    replaceAmount: function (amount) {\n      // 1,000のようにカンマがある状態でも変更できるようにする関数\n      if (amount.match(/,/)) {\n        var amount = amount.replace(\",\", \"\");\n      }\n      return amount;\n    },\n    setBarData: function () {\n      this.barData = {\n        labels: ['収入', '支出'],\n        datasets: [{\n          label: 'Data',\n          data: this.balanceBarData,\n          backgroundColor: '#f87979'\n        }]\n      };\n    },\n    updateGraph: function () {\n      var outTotal = 0;\n      var inTotal = 0;\n      for (var index = 0; index < this.oeconomicas.length; index++) {\n        var amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n        if (this.oeconomicas[index]['balance'] == \"支出\") {\n          var outTotal = outTotal + Number(amount);\n        } else if (this.oeconomicas[index]['balance'] == \"収入\") {\n          var inTotal = inTotal + Number(amount);\n        }\n      }\n      this.balanceBarData = [inTotal, outTotal];\n      this.setBarData();\n      // const ctx = document.getElementById('myChart').getContext('2d');\n      const canvas = document.getElementById('myChart');\n      // if (canvas.chart) {\n      // Destroy the existing chart instance\n      console.log(canvas);\n      canvas.chart.destroy();\n      // }\n      // Create a new chart instance\n      const ctx = document.getElementById('myChart').getContext('2d');\n      const myChart = new Chart(ctx, {\n        type: 'bar',\n        data: this.barData,\n        options: this.options\n      });\n    }\n  }\n};","map":{"version":3,"names":["Chart","registerables","register","name","props","data","balanceBarData","barData","options","responsive","indexAxis","maintainAspectRatio","created","outTotal","inTotal","index","oeconomicas","length","amount","replaceAmount","Number","setBarData","console","log","mounted","ctx","document","getElementById","getContext","myChart","type","methods","match","replace","labels","datasets","label","backgroundColor","updateGraph","canvas","chart","destroy"],"sources":["/app/src/components/Graph.vue"],"sourcesContent":["<template>\n    <h2 id=\"balanceBar\">収支グラフ</h2>\n    <div class=\"test\">\n        <!-- <input type=\"button\" value=\"check\" @click=\"checkProps\" > -->\n        <p>{{ oeconomicas }}</p>\n        <p>{{ year_month }}</p>\n    </div>\n    <canvas id=\"myChart\" width=\"1000\" height=\"200\"></canvas>\n\n</template>\n\n<script>\nimport { Chart, registerables } from 'chart.js'; \nChart.register(...registerables);\nexport default {\n    name: 'Graph',\n    props: ['oeconomicas','year_month'],\n    data () {\n        return {\n            balanceBarData: [],\n            barData: {},\n            options: {\n                responsive: false,\n                indexAxis: 'y',\n                maintainAspectRatio: false,\n                // scales: {\n                //     yAxes: [{\n                //         ticks: {\n                //         beginAtZero: true,\n                //         },\n                //     }],\n                // },\n            },    \n        }\n    },\n    created:function () {\n        var outTotal = 0;\n        var inTotal = 0;\n        for(var index=0; index < this.oeconomicas.length; index++) {\n            var amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n            if ( this.oeconomicas[index]['balance'] == \"支出\" ) {\n                var outTotal = outTotal + Number(amount);\n            } else if ( this.oeconomicas[index]['balance'] == \"収入\" ) {\n                var inTotal = inTotal + Number(amount);\n            }\n        }\n        this.balanceBarData = [inTotal, outTotal];\n        this.setBarData();\n        console.log(this.barData);\n    },\n    mounted() {\n        const ctx = document.getElementById('myChart').getContext('2d');\n        this.myChart = new Chart(ctx, {\n            type: 'bar',\n            data: this.barData,\n            options: this.options,\n        });\n    },\n    methods: {\n        replaceAmount: function(amount) {\n            // 1,000のようにカンマがある状態でも変更できるようにする関数\n            if ( amount.match(/,/)) {\n                var amount = amount.replace(\",\", \"\");\n            } \n            return amount;\n        },\n        setBarData: function(){\n            this.barData = {\n                labels: ['収入', '支出'],\n                datasets: [\n                    {\n                        label: 'Data',\n                        data: this.balanceBarData,\n                        backgroundColor: '#f87979'\n                    }\n                ]\n            }\n        },\n        updateGraph: function() {\n            var outTotal = 0;\n            var inTotal = 0;\n            for(var index=0; index < this.oeconomicas.length; index++) {\n                var amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n                if ( this.oeconomicas[index]['balance'] == \"支出\" ) {\n                    var outTotal = outTotal + Number(amount);\n                } else if ( this.oeconomicas[index]['balance'] == \"収入\" ) {\n                    var inTotal = inTotal + Number(amount);\n                }\n            }\n            this.balanceBarData = [inTotal, outTotal];\n            this.setBarData();\n            // const ctx = document.getElementById('myChart').getContext('2d');\n            const canvas = document.getElementById('myChart')\n            // if (canvas.chart) {\n                // Destroy the existing chart instance\n                console.log(canvas);\n                canvas.chart.destroy();\n            // }\n            // Create a new chart instance\n            const ctx = document.getElementById('myChart').getContext('2d');\n            const myChart = new Chart(ctx, {\n                type: 'bar',\n                data: this.barData,\n                options: this.options,\n            });\n        },\n    }\n}\n</script>"],"mappings":"AAYA,SAASA,KAAK,EAAEC,aAAY,QAAS,UAAU;AAC/CD,KAAK,CAACE,QAAQ,CAAC,GAAGD,aAAa,CAAC;AAChC,eAAe;EACXE,IAAI,EAAE,OAAO;EACbC,KAAK,EAAE,CAAC,aAAa,EAAC,YAAY,CAAC;EACnCC,IAAGA,CAAA,EAAK;IACJ,OAAO;MACHC,cAAc,EAAE,EAAE;MAClBC,OAAO,EAAE,CAAC,CAAC;MACXC,OAAO,EAAE;QACLC,UAAU,EAAE,KAAK;QACjBC,SAAS,EAAE,GAAG;QACdC,mBAAmB,EAAE;QACrB;QACA;QACA;QACA;QACA;QACA;QACA;MACJ;IACJ;EACJ,CAAC;;EACDC,OAAO,EAAC,SAAAA,CAAA,EAAY;IAChB,IAAIC,QAAO,GAAI,CAAC;IAChB,IAAIC,OAAM,GAAI,CAAC;IACf,KAAI,IAAIC,KAAK,GAAC,CAAC,EAAEA,KAAI,GAAI,IAAI,CAACC,WAAW,CAACC,MAAM,EAAEF,KAAK,EAAE,EAAE;MACvD,IAAIG,MAAK,GAAI,IAAI,CAACC,aAAa,CAAC,IAAI,CAACH,WAAW,CAACD,KAAK,CAAC,CAAC,QAAQ,CAAC,CAAC;MAClE,IAAK,IAAI,CAACC,WAAW,CAACD,KAAK,CAAC,CAAC,SAAS,KAAK,IAAG,EAAI;QAC9C,IAAIF,QA