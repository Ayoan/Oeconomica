{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport axios from \"axios\";\nimport tFilterInit from './filter.js';\nimport Graph from './Graph.vue';\nexport default {\n  name: 'MyMoney',\n  components: {\n    Graph\n  },\n  data() {\n    return {\n      year_month: this.showDate(\"m\"),\n      date: this.showDate(\"d\"),\n      min_date: this.showDate(\"Min\"),\n      max_date: this.showDate(\"Max\"),\n      balance: '支出',\n      show: \"1\",\n      category: \"-選択してください-\",\n      expenceCategories: [\"食費\", \"酒代\", \"日用品\", \"交際費\", \"交通費\", \"美容費\", \"衣服費\", \"医療費\", \"書籍\", \"サブスク\", \"家賃\", \"水道光熱費\", \"通信費\", \"税金\", \"家具・家電\", \"サウナ・銭湯\", \"車代\", \"旅行\", \"その他\"],\n      incomeCategories: [\"給与\", \"投資収入\", \"その他\"],\n      amount: \"\",\n      memo: \"\",\n      oeconomicas: [],\n      graphFinished: 0\n    };\n  },\n  created: async function () {\n    try {\n      const result = await axios.get(\"/oeconomica/\", {\n        params: {\n          year_month: this.year_month\n        }\n      });\n      this.oeconomicas = result.data;\n      // this.$refs.child.firstAction(this.oeconomicas,[this.expenceCategories,this.incomeCategories]);\n    } catch (err) {\n      console.log(\"error catch\");\n      console.log(err);\n      alert(JSON.stringify(err));\n    }\n  },\n  mounted() {\n    this.$refs.child.firstAction(this.oeconomicas, [this.expenceCategories, this.incomeCategories]);\n    const script = document.createElement('script');\n    script.setAttribute('src', './filter.js');\n    script.setAttribute('id', 'filter_script_id');\n    document.head.appendChild(script);\n    tFilterInit();\n  },\n  methods: {\n    select: function (num) {\n      if (num == 2) {\n        this.$refs.child.firstAction(this.oeconomicas, [this.expenceCategories, this.incomeCategories]);\n      }\n      this.show = num;\n    },\n    resetSelect: function () {\n      this.category = \"-選択してください-\";\n    },\n    showDate: function (str_flag) {\n      const dt = new Date();\n      var y = dt.getFullYear();\n      var m = ('00' + (dt.getMonth() + 1)).slice(-2);\n      var d = ('00' + dt.getDate()).slice(-2);\n      if (str_flag == \"d\") {\n        return y + '-' + m + '-' + d;\n      } else if (str_flag == \"m\") {\n        return y + '-' + m;\n      } else if (str_flag == \"Min\") {\n        return y + '-' + m + '-01';\n      } else if (str_flag == \"Max\") {\n        var getsumatsu = new Date(dt.getFullYear(), dt.getMonth() + 1, 0);\n        var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n        return y + '-' + m + '-' + end_date;\n      }\n    },\n    setDateRange: async function (year_month) {\n      try {\n        var min_date = year_month + '-01';\n        const dt = new Date();\n        var getsumatsu = new Date(dt.getFullYear(), dt.getMonth(), 0);\n        var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n        var max_date = year_month + '-' + end_date;\n        this.min_date = min_date;\n        this.max_date = max_date;\n        const result = await axios.get(\"/oeconomica/\", {\n          params: {\n            year_month: this.year_month\n          }\n        });\n        this.oeconomicas = result.data;\n        // this.$refs.child.updateGraph(this.oeconomicas);\n        this.$refs.child.firstAction(this.oeconomicas, [this.expenceCategories, this.incomeCategories]);\n      } catch (err) {\n        console.log(\"error catch\");\n        console.log(err);\n        alert(JSON.stringify(err));\n      }\n    },\n    regist: async function () {\n      try {\n        const result = await axios.post(\"/oeconomica/\", {\n          date: this.date,\n          balance: this.balance,\n          category: this.category,\n          amount: this.amount,\n          memo: this.memo\n        });\n        this.oeconomicas.unshift(result.data); //配列の先頭に要素追加\n        this.modifyNewRecord();\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    deleteData: async function (id, index) {\n      try {\n        await axios.delete(\"/oeconomica/\" + id);\n        this.oeconomicas.splice(index, 1);\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    changeData: async function (id, index) {\n      let val_date = this.oeconomicas[index]['date'];\n      let val_balance = this.oeconomicas[index]['balance'];\n      let val_category = this.oeconomicas[index]['category'];\n      let val_amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n      let val_memo = this.oeconomicas[index]['memo'];\n\n      // 更新しようとするカテゴリが規定のものと一致するかチェック\n      var isMatched = \"\";\n      if (val_balance == '収入') {\n        isMatched = this.incomeCategories.includes(val_category);\n      } else if (val_balance == '支出') {\n        isMatched = this.expenceCategories.includes(val_category);\n      } else {\n        alert(JSON.stringify(\"収入/支出以外のデータは更新できません\"));\n        return;\n      }\n      if (isMatched) {\n        console.log(\"カテゴリチェックOK\");\n      } else {\n        alert(JSON.stringify(\"規定外のカテゴリが入力されています。\"));\n        return;\n      }\n      try {\n        await axios.put(\"/oeconomica/\" + id, {\n          date: val_date,\n          balance: val_balance,\n          category: val_category,\n          amount: val_amount,\n          memo: val_memo\n        });\n        // 曜日も更新する\n        var wDay = this.getWDay(val_date);\n        this.oeconomicas[index]['day_of_week'] = wDay;\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    replaceAmount: function (amount) {\n      // 1,000のようにカンマがある状態でも変更できるようにする関数\n      if (amount.match(/,/)) {\n        amount = amount.replace(\",\", \"\");\n      }\n      return amount;\n    },\n    csAmount: function (amount) {\n      return amount.toLocaleString();\n    },\n    getWDay: function (varDate) {\n      // 曜日を表す文字列の配列\n      var WeekChars = [\"日\", \"月\", \"火\", \"水\", \"木\", \"金\", \"土\"];\n      var strDateArray = varDate.split(\"-\");\n      var strDate = strDateArray[0] + '/' + strDateArray[1] + '/' + strDateArray[2];\n      var dObj = new Date(strDate);\n      var wDay = dObj.getDay();\n      return WeekChars[wDay];\n    },\n    modifyNewRecord: function () {\n      // 新規登録したデータの画面表記を修正する\n      var wDay = this.getWDay(this.date); // 日付から曜日を特定\n      var strAmount = this.csAmount(this.amount); // 金額にカンマをつける\n      // 画面反映\n      this.oeconomicas[0].day_of_week = wDay;\n      this.oeconomicas[0]['amount'] = strAmount;\n    }\n  }\n};","map":{"version":3,"names":["axios","tFilterInit","Graph","name","components","data","year_month","showDate","date","min_date","max_date","balance","show","category","expenceCategories","incomeCategories","amount","memo","oeconomicas","graphFinished","created","result","get","params","err","console","log","alert","JSON","stringify","mounted","$refs","child","firstAction","script","document","createElement","setAttribute","head","appendChild","methods","select","num","resetSelect","str_flag","dt","Date","y","getFullYear","m","getMonth","slice","d","getDate","getsumatsu","end_date","setDateRange","regist","post","unshift","modifyNewRecord","deleteData","id","index","delete","splice","changeData","val_date","val_balance","val_category","val_amount","replaceAmount","val_memo","isMatched","includes","put","wDay","getWDay","match","replace","csAmount","toLocaleString","varDate","WeekChars","strDateArray","split","strDate","dObj","getDay","strAmount","day_of_week"],"sources":["/app/src/components/Oeconomica.vue"],"sourcesContent":["<template>\n    <header>\n        <div id = mymoney>\n        <form name=\"login_form\">\n            <div class=\"login_form_top\">\n                <h1>Oeconomica</h1>\n                <p>Simple Account Book made by myself</p>\n            </div>\n            <div class=\"login_form_btm\">\n                <input type=\"month\" v-model=\"year_month\" v-on:change=\"setDateRange(year_month)\"/><br>\n            </div>\n        </form>\n        <div id=\"tab-select\">\n     