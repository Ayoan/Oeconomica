ategories;\n        //     this.expenceCategories = expenceCategories;\n        // },\n        initializeData: function() {\n            let data = {};\n            for (let i = 1; i <= 12; i++) {\n                data[i] = {};\n                for (let j = 0; j < this.incomeCategories.length; j++) {\n                    data[i][category] = 0;\n                }\n            }\n            return data;\n        },\n    },\n}\n\n</script>"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,KAAK,EAAEC,aAAY,QAAS,UAAU;AAC/CD,KAAK,CAACE,QAAQ,CAAC,GAAGD,aAAa,CAAC;AAChC,eAAe;EACXE,IAAI,EAAE,WAAW;EACjBC,IAAGA,CAAA,EAAK;IACJ,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,gBAAgB,EAAE,CAAC,IAAI,EAAE,MAAM,EAAC,KAAK,CAAC;MACtCC,iBAAiB,EAAE,CAAC,IAAI,EAAC,IAAI,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,IAAI,EAAC,MAAM,EAAC,IAAI,EAAC,OAAO,EAAC,KAAK,EAAC,IAAI,EAAC,OAAO,EAAC,QAAQ,EAAC,IAAI,EAAC,IAAI,EAAC,KAAK,CAAC;MACvIC,aAAa,EAAE,IAAI,CAACC,cAAc,EAAE;MACpCC,cAAc,EAAE,CAAC;IAErB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,YAAY,EAAE,eAAAA,CAAeC,IAAI,EAAE;MAC/B,IAAI;QACA,MAAMC,MAAK,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,uBAAuB,EAAC;UACnDC,MAAM,EAAE;YACJC,IAAI,EAAEJ;UACV;QACJ,CAAC,CAAC;QACF,IAAI,CAACL,aAAY,GAAIU,aAAa,CAACJ,MAAM,CAACV,IAAI,EAAC,IAAI,CAACE,gBAAgB,EAAC,IAAI,CAAC;QAC1E,IAAI,CAACI,cAAa,GAAIQ,aAAa,CAACJ,MAAM,CAACV,IAAI,EAAC,IAAI,CAACG,iBAAiB,EAAC,IAAI,CAAC;QAC5EY,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,aAAa,CAAC;MACnC,EAAE,OAAOa,GAAG,EAAE;QACVF,OAAO,CAACC,GAAG,CAAC,aAAa,CAAC;QAC1BD,OAAO,CAACC,GAAG,CAACC,GAAG,CAAC;QAChBC,KAAK,CAACC,IAAI,CAACC,SAAS,CAACH,GAAG,CAAC,CAAC;MAC9B;IACJ,CAAC;IACD;IACA;IACA;IACA;IACAZ,cAAc,EAAE,SAAAA,CAAA,EAAW;MACvB,IAAIL,IAAG,GAAI,CAAC,CAAC;MACb,KAAK,IAAIqB,CAAA,GAAI,CAAC,EAAEA,CAAA,IAAK,EAAE,EAAEA,CAAC,EAAE,EAAE;QAC1BrB,IAAI,CAACqB,CAAC,IAAI,CAAC,CAAC;QACZ,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACpB,gBAAgB,CAACqB,MAAM,EAAED,CAAC,EAAE,EAAE;UACnDtB,IAAI,CAACqB,CAAC,CAAC,CAACG,QAAQ,IAAI,CAAC;QACzB;MACJ;MACA,OAAOxB,IAAI;IACf;EACJ;AACJ"},"metadata":{},"sourceType