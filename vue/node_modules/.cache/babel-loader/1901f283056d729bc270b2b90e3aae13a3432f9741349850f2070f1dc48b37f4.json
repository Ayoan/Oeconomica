{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport axios from \"axios\";\nimport Graph from './Graph.vue';\nexport default {\n  name: 'MyMoney',\n  components: {\n    Graph\n  },\n  data() {\n    return {\n      year_month: this.showDate(\"m\"),\n      date: this.showDate(\"d\"),\n      min_date: this.showDate(\"Min\"),\n      max_date: this.showDate(\"Max\"),\n      balance: '支出',\n      show: \"1\",\n      category: \"-選択してください-\",\n      expenceCategories: [\"食費\", \"酒代\", \"日用品\", \"交際費\", \"交通費\", \"美容費\", \"衣服費\", \"医療費\", \"書籍\", \"サブスク\", \"家賃\", \"水道光熱費\", \"通信費\", \"税金\", \"家具・家電\", \"サウナ・銭湯\", \"車代\", \"旅行\", \"その他\"],\n      incomeCategories: [\"給与\", \"投資収入\", \"その他\"],\n      amount: \"\",\n      memo: \"\",\n      oeconomicas: []\n    };\n  },\n  created: async function () {\n    try {\n      const result = await axios.get(\"/oeconomica/\", {\n        params: {\n          year_month: this.year_month\n        }\n      });\n      this.oeconomicas = result.data;\n      this.$refs.child.firstAction(this.oeconomicas);\n    } catch (err) {\n      console.log(\"error catch\");\n      console.log(err);\n      alert(JSON.stringify(err));\n    }\n  },\n  methods: {\n    select: function (num) {\n      this.show = num;\n    },\n    resetSelect: function () {\n      this.category = \"-選択してください-\";\n    },\n    showDate: function (str_flag) {\n      const dt = new Date();\n      var y = dt.getFullYear();\n      var m = ('00' + (dt.getMonth() + 1)).slice(-2);\n      var d = ('00' + dt.getDate()).slice(-2);\n      if (str_flag == \"d\") {\n        return y + '-' + m + '-' + d;\n      } else if (str_flag == \"m\") {\n        return y + '-' + m;\n      } else if (str_flag == \"Min\") {\n        return y + '-' + m + '-01';\n      } else if (str_flag == \"Max\") {\n        var getsumatsu = new Date(dt.getFullYear(), dt.getMonth() + 1, 0);\n        var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n        return y + '-' + m + '-' + end_date;\n      }\n    },\n    setDateRange: async function (year_month) {\n      try {\n        var min_date = year_month + '-01';\n        const dt = new Date();\n        var getsumatsu = new Date(dt.getFullYear(), dt.getMonth(), 0);\n        var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n        var max_date = year_month + '-' + end_date;\n        this.min_date = min_date;\n        this.max_date = max_date;\n        const result = await axios.get(\"/oeconomica/\", {\n          params: {\n            year_month: this.year_month\n          }\n        });\n        this.oeconomicas = result.data;\n        this.$refs.child.updateGraph(this.oeconomicas);\n      } catch (err) {\n        console.log(\"error catch\");\n        console.log(err);\n        alert(JSON.stringify(err));\n      }\n    },\n    regist: async function () {\n      try {\n        // 曜日を表す文字列の配列\n        var WeekChars = [\"日\", \"月\", \"火\", \"水\", \"木\", \"金\", \"土\"];\n        var strDateArray = this.date.split(\"-\");\n        var strDate = strDateArray[0] + '/' + strDateArray[1] + '/' + strDateArray[2];\n        var dObj = new Date(strDate);\n        var wDay = dObj.getDay();\n        const result = await axios.post(\"/oeconomica/\", {\n          date: this.date,\n          balance: this.balance,\n          category: this.category,\n          amount: this.amount,\n          memo: this.memo\n        });\n        // this.oeconomicas.push(result.data);\n        this.oeconomicas.unshift(result.data);\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    deleteData: async function (id, index) {\n      try {\n        await axios.delete(\"/oeconomica/\" + id);\n        this.oeconomicas.splice(index, 1);\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    changeData: async function (id, index) {\n      for (const key in this.oeconomicas[index]) {\n        console.log(`キー[${key}]値:[${this.oeconomicas[index][key]}]`);\n      }\n      let val_date = this.oeconomicas[index]['date'];\n      let val_balance = this.oeconomicas[index]['balance'];\n      let val_category = this.oeconomicas[index]['category'];\n      let val_amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n      let val_memo = this.oeconomicas[index]['memo'];\n      try {\n        await axios.put(\"/oeconomica/\" + id, {\n          date: val_date,\n          balance: val_balance,\n          category: val_category,\n          amount: val_amount,\n          memo: val_memo\n        });\n      } catch (err) {\n        alert(JSON.stringify(err));\n      }\n    },\n    replaceAmount: function (amount) {\n      // 1,000のようにカンマがある状態でも変更できるようにする関数\n      if (amount.match(/,/)) {\n        amount = amount.replace(\",\", \"\");\n      }\n      return amount;\n    }\n  }\n};","map":{"version":3,"names":["axios","Graph","name","components","data","year_month","showDate","date","min_date","max_date","balance","show","category","expenceCategories","incomeCategories","amount","memo","oeconomicas","created","result","get","params","$refs","child","firstAction","err","console","log","alert","JSON","stringify","methods","select","num","resetSelect","str_flag","dt","Date","y","getFullYear","m","getMonth","slice","d","getDate","getsumatsu","end_date","setDateRange","updateGraph","regist","WeekChars","strDateArray","split","strDate","dObj","wDay","getDay","post","unshift","deleteData","id","index","delete","splice","changeData","key","val_date","val_balance","val_category","val_amount","replaceAmount","val_memo","put","match","replace"],"sources":["/app/src/components/Oeconomica.vue"],"sourcesContent":["<template>\n    <header>\n        <div id = mymoney>\n        <form name=\"login_form\">\n            <div class=\"login_form_top\">\n                <h1>Oeconomica</h1>\n                <p>Simple Account Book made by myself</p>\n            </div>\n            <div class=\"login_form_btm\">\n                <input type=\"month\" v-model=\"year_month\" v-on:change=\"setDateRange(year_month)\"/><br>\n            </div>\n        </form>\n        <div id=\"tab-select\">\n            <div class=\"tabgroup\">\n              <ul class=\"tabnav\">\n                <li @click=\"select('1')\" v-bind:class=\"{'active': show == '1'}\">\n                    入力\n                </li>\n                <li @click=\"select('2')\" v-bind:class=\"{'active': show == '2'}\">\n                    データ\n                </li>\n              </ul>\n              <div class=\"tabcontent\">\n                <div v-show=\"show == '1'\" class=\"tabcontent-list\">\n                    <article>\n                        <!-- 入力 -->\n                        <h2 id=\"input\">入力</h2>\n                        <section id=\"inputSection\">\n                            <div>\n                                <label>収支</label>\n                                    <input type=\"radio\" value=\"支出\" v-model=\"balance\" @click=\"resetSelect\"/>支出\n                                    <input type=\"radio\" value=\"収入\" v-model=\"balance\" @click=\"resetSelect\"/>収入\n\n                                <label>日付</label>\n                                    <input type=\"date\" v-model=\"date\" v-bind:min=\"min_date\" v-bind:max=\"max_date\">\n                                    <!-- <input type=\"date\" v-model=\"date\"> -->\n                                <label>カテゴリ</label>\n                \n                                <div v-if=\"balance == '支出'\" class=\"optionGroup\">\n                                    <select v-model=\"category\">\n                                        <option>-選択してください-</option>                            \n                                        <option v-for=\"(cg, index) in expenceCategories\" :key=\"index\" v-bind:value=\"cg\">{{ cg }}</option>\n                                    </select>\n                                </div>\n                                <div v-else-if=\"balance == '収入'\" class=\"optionGroup\">\n                                    <select v-model=\"category\">\n                                        <option>-選択してください-</option>\n                                        <option v-for=\"(cg, index) in incomeCategories\" :key=\"index\" v-bind:value=\"cg\">{{ cg }}</option>\n                                    </select>\n                                </div>\n                                \n                            </div>\n                            <div>\n                                <label>金額</label>\n                                    <input type=\"number\" placeholder=\"金額を記入\" v-model=\"amount\">円\n\n                                <label>メモ</label>\n                                <input type=\"text\" v-model=\"memo\" placeholder=\"買ったものを記入\">\n                            </div>\n                        </section>\n                        <div class=\"submit\">\n                            <input type=\"button\" value=\"登録する\" @click=\"regist\" >\n                        </div>\n                    </article>\n\n                        <!-- 入出金一覧 -->\n                        <h2 id=\"inOutList\">入出金一覧</h2>\n                        <section id=\"list\">\n                            <!-- ここに入出金一覧を表示します。 -->\n                            <table class=\"inOutTable\">\n                                <tr>\n                                    <th width=\"38\">曜日</th>\n                                    <th>日付</th>\n                                    <th>収支</th>\n                                    <th>カテゴリ</th>\n                                    <th>金額(円)</th>\n                                    <th>メモ</th>\n                                    <th width=\"80\">変更</th>\n                                    <th width=\"80\">削除</th>\n                                </tr>\n                                <tr v-for=\"(oeconomica, index) in oeconomicas\" :key=\"oeconomica.id\">\n                                    <td id=\"day\">{{ oeconomica.day_of_week }}</td>\n                                    <td><input type=\"text\" v-model=\"oeconomica.date\"/></td>\n                                    <td><input type=\"text\" v-model=\"oeconomica.balance\"/></td>\n                                    <td><input type=\"text\" v-model=\"oeconomica.category\"/></td>\n                                    <td><input type=\"text\" v-model=\"oeconomica.amount\"/></td>\n                                    <td><input type=\"text\" v-model=\"oeconomica.memo\"/></td>\n                                    <td><input type=\"button\" value=\"change\" @click=\"changeData(oeconomica.id, index)\"/></td>\n                                    <td><input type=\"button\" value=\"delete\" @click=\"deleteData(oeconomica.id, index)\"/></td>\n                                </tr>\n                            </table>\n                        </section>  \n                </div>\n\n                <div v-show=\"show == '2'\" class=\"tabcontent-list\">\n                    <div class=\"graph-child\">\n                        <graph ref=\"child\"></graph>\n                    </div>\n                </div>\n              </div>    \n            </div>\n        </div>\n    </div>\n    </header>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Graph from './Graph.vue'\n\nexport default {\n    name: 'MyMoney',\n    components: {\n        Graph\n    },\n    data () {\n      return {\n        year_month: this.showDate(\"m\"), \n        date: this.showDate(\"d\"),\n        min_date: this.showDate(\"Min\"),\n        max_date: this.showDate(\"Max\"),\n        balance: '支出',\n        show: \"1\",\n        category: \"-選択してください-\" ,\n        expenceCategories: [\"食費\",\"酒代\",\"日用品\",\"交際費\",\"交通費\",\"美容費\",\"衣服費\",\"医療費\",\"書籍\",\"サブスク\",\"家賃\",\"水道光熱費\",\"通信費\",\"税金\",\"家具・家電\",\"サウナ・銭湯\",\"車代\",\"旅行\",\"その他\"],\n        incomeCategories: [\"給与\", \"投資収入\",\"その他\"],\n        amount: \"\",\n        memo: \"\",\n        oeconomicas: [],\n        \n      }\n    },\n    created: async function () {\n        try {\n            const result = await axios.get(\"/oeconomica/\",{\n                params: {\n                    year_month: this.year_month\n                }\n            });\n            this.oeconomicas = result.data;\n            this.$refs.child.firstAction(this.oeconomicas);\n        } catch (err) {\n            console.log(\"error catch\");\n            console.log(err);\n            alert(JSON.stringify(err));\n        }\n    },\n    methods: {\n        select: function (num) {\n            this.show = num;\n        },\n        resetSelect: function() {\n            this.category = \"-選択してください-\";\n        },\n        showDate: function (str_flag) {\n            const dt = new Date();\n            var y = dt.getFullYear();\n            var m = ('00' + (dt.getMonth()+1)).slice(-2);\n            var d = ('00' + dt.getDate()).slice(-2);\n            if ( str_flag == \"d\" ) {\n                return (y + '-' + m + '-' + d);\n            } else if ( str_flag == \"m\" ) {\n                return (y + '-' + m);\n            } else if ( str_flag == \"Min\" ) {\n                return (y + '-' + m + '-01');\n            } else if ( str_flag == \"Max\") {\n                var getsumatsu = new Date(dt.getFullYear(), dt.getMonth() + 1, 0);\n                var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n                return (y + '-' + m + '-' + end_date);\n            }\n        },\n        setDateRange: async function(year_month) {\n            try {\n                var min_date = year_month + '-01';\n                const dt = new Date();\n                var getsumatsu = new Date(dt.getFullYear(), dt.getMonth(), 0);\n                var end_date = ('00' + getsumatsu.getDate()).slice(-2);\n                var max_date = year_month + '-' + end_date;\n                this.min_date = min_date;\n                this.max_date = max_date;\n                const result = await axios.get(\"/oeconomica/\",{\n                    params: {\n                        year_month: this.year_month\n                    }\n                });\n            this.oeconomicas = result.data;\n            this.$refs.child.updateGraph(this.oeconomicas);\n            } catch (err) {\n                console.log(\"error catch\");\n                console.log(err);\n                alert(JSON.stringify(err));\n            }\n        },\n        regist: async function () {\n            try {\n                // 曜日を表す文字列の配列\n                var WeekChars = [ \"日\", \"月\", \"火\", \"水\", \"木\", \"金\", \"土\" ];\n                var strDateArray = this.date.split(\"-\");\n                var strDate = strDateArray[0] + '/' + strDateArray[1] + '/' + strDateArray[2];\n                var dObj = new Date(strDate);\n                var wDay = dObj.getDay();\n                \n                const result = await axios.post(\"/oeconomica/\", {\n                    date: this.date,\n                    balance: this.balance,\n                    category: this.category,\n                    amount: this.amount,\n                    memo: this.memo\n                });\n                // this.oeconomicas.push(result.data);\n                this.oeconomicas.unshift(result.data);\n            } catch (err) {\n                alert(JSON.stringify(err));\n            }\n        },\n        deleteData: async function (id, index) {\n            try {\n                await axios.delete(\"/oeconomica/\" + id);\n                this.oeconomicas.splice(index, 1);\n            } catch (err) {\n                alert(JSON.stringify(err));\n            }\n        },\n        changeData: async function (id, index) {\n            for (const key in this.oeconomicas[index]) {\n                console.log(`キー[${key}]値:[${this.oeconomicas[index][key]}]`);\n            }\n            let val_date = this.oeconomicas[index]['date'];\n            let val_balance = this.oeconomicas[index]['balance'];\n            let val_category = this.oeconomicas[index]['category'];\n            let val_amount = this.replaceAmount(this.oeconomicas[index]['amount']);\n            let val_memo = this.oeconomicas[index]['memo'];\n            try {\n                await axios.put(\"/oeconomica/\" + id, {\n                    date: val_date,\n                    balance: val_balance,\n                    category: val_category,\n                    amount: val_amount,\n                    memo: val_memo\n                });\n            } catch (err) {\n                alert(JSON.stringify(err));\n            }\n        },\n        replaceAmount: function(amount) {\n            // 1,000のようにカンマがある状態でも変更できるようにする関数\n            if ( amount.match(/,/)) {\n                amount = amount.replace(\",\", \"\");\n            } \n            return amount;\n        },\n    }\n}\n</script>\n<style>\nform {\n    text-align: center;\n}\n\n.login_form_btm {\n    padding: 10px 0;\n    background-color: #F7F7F7;\n    margin: 10px 0;\n}\n</style>"],"mappings":";AA2GA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,aAAY;AAE9B,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,UAAU,EAAE,IAAI,CAACC,QAAQ,CAAC,GAAG,CAAC;MAC9BC,IAAI,EAAE,IAAI,CAACD,QAAQ,CAAC,GAAG,CAAC;MACxBE,QAAQ,EAAE,IAAI,CAACF,QAAQ,CAAC,KAAK,CAAC;MAC9BG,QAAQ,EAAE,IAAI,CAACH,QAAQ,CAAC,KAAK,CAAC;MAC9BI,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,GAAG;MACTC,QAAQ,EAAE,YAAW;MACrBC,iBAAiB,EAAE,CAAC,IAAI,EAAC,IAAI,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,KAAK,EAAC,IAAI,EAAC,MAAM,EAAC,IAAI,EAAC,OAAO,EAAC,KAAK,EAAC,IAAI,EAAC,OAAO,EAAC,QAAQ,EAAC,IAAI,EAAC,IAAI,EAAC,KAAK,CAAC;MACvIC,gBAAgB,EAAE,CAAC,IAAI,EAAE,MAAM,EAAC,KAAK,CAAC;MACtCC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE;IAEf;EACF,CAAC;EACDC,OAAO,EAAE,eAAAA,CAAA,EAAkB;IACvB,IAAI;MACA,MAAMC,MAAK,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,cAAc,EAAC;QAC1CC,MAAM,EAAE;UACJhB,UAAU,EAAE,IAAI,CAACA;QACrB;MACJ,CAAC,CAAC;MACF,IAAI,CAACY,WAAU,GAAIE,MAAM,CAACf,IAAI;MAC9B,IAAI,CAACkB